
  <h1><strong>{{titlea}}</strong> {{titleb}}</h1>

  <div class="pt-triggers">

    {{#if input}}
      {{#input}}
          <input id="{{id}}" type="text" name="{{id}}">
      {{/input}}
    {{/if}}

    {{#if users}}
      <ul id="users"></ul>
    {{/if}}

    {{#if spinner}}
      <div class="spinner sk-rotating-plane"></div>
    {{/if}}

    {{#if buttons}}
      {{#buttons}}
        <button id="{{id}}" class="pt-touch-button {{class}}"
        {{#if data}}
          {{#data}}
            data-{{name}}="{{value}}"
          {{/data}}
        {{/if}}
        >{{title}}</button>
      {{/buttons}}
    {{/if}}


  </div>
